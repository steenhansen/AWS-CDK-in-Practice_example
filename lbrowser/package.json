{
  "name": "lbrowser",
  "version": "1.0.0",
  "license": "MIT",
  "LOCAL": {
    "PORT_BROWSER": "3004"
  },
  "jest": {
    "moduleNameMapper": {
      "^axios$": "axios/dist/node/axios.cjs"
    }
  },
  "scripts": {
    "build-local": "cross-env react-scripts build",
    "start-local": "gulp && cross-env  PORT=$npm_package_LOCAL_PORT_BROWSER  REACT_APP__LOCAL_MODE=yes  react-scripts start",
    "build-prod": "cross-env REACT_ENV=Prod react-scripts build",
    "build-dev": "cross-env REACT_ENV=Dev react-scripts build",
    "test": "react-scripts test --watchAll=false",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.4.1",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "axios": "^1.7.4",
    "cross-env": "^7.0.3",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "react-scripts": "5.0.1",
    "styled-components": "^6.1.12",
    "typescript": "^5.5.4",
    "web-vitals": "^4.2.3"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.0",
    "@testing-library/dom": "^10.4.0",
    "@types/styled-components": "^5.1.34",
    "gulp": "^5.0.0"
  }
}