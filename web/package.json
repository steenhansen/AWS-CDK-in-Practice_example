    "@typescript-eslint/eslint-plugin": "^8.6.0",
    "@typescript-eslint/parser": "^8.6.0",
    "@typescript-eslint/typescript-estree": "^8.6.0",
    "eslint": "^9.11.0",
    "eslint-config-react-app": "^7.0.1",
    "gulp": "^5.0.0",
    "jest": "^29.7.0",
