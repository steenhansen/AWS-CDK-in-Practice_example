{
  "name": "lbrowser",
  "version": "1.0.0",
  "license": "MIT",
  "LOCAL_VAR": {
    "PORT_BROWSER": "3004"
  },
  "scripts": {
    "web-build-prod": "cross-env  REACT_APP_ENV=Env_prd  craco build",
    "web-build-dev": " cross-env  REACT_APP_ENV=Env_dvl  craco build",
    "web-start": "cross-env PORT=$npm_package_LOCAL_VAR_PORT_BROWSER  REACT_APP__LOCAL_MODE=yes  craco  start",
    "web-test": "gulp && cross-env  REACT_APP__LOCAL_MODE=yes craco  test  --watchAll=false --verbose=true",
    "eject": "craco  eject"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.4.1",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "cross-env": "^7.0.3",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "react-scripts": "5.0.1",
    "styled-components": "^6.1.12",
    "typescript": "^5.5.4",
    "web-vitals": "^4.2.3"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.0",
    "@craco/craco": "^7.1.0",
    "@testing-library/dom": "^10.4.0",
    "@types/styled-components": "^5.1.34",
    "gulp": "^5.0.0"
  }
}