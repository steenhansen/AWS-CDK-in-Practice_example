  "license": "MIT",
  "jest": {
    "moduleNameMapper": {
      "^axios$": "axios/dist/node/axios.cjs"
    }
  },
  "resolutions": {
    "@typescript-eslint/typescript-estree": "^5.59.2"
  },
  "LOCAL": {
    "PORT_BROWSER": "3004",
    "LSECRETS_BROWSER": "../../aws-cdk-in-practice_secrets.json"
  },
  "scripts": {
    "web-start": "cross-env PORT=$npm_package_LOCAL_PORT_BROWSER  REACT_APP__LOCAL_MODE=yes  craco  start",
    "web-build-prod": "cross-env REACT_APP_ENV=Production craco build",
    "web-build-dev": "cross-env REACT_APP_ENV=Development craco build",
    "test": "react-scripts  test --watchAll=false",
    "eject": "react-scripts  eject"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.4.1",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "axios": "^1.7.4",
    "cross-env": "^7.0.3",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "react-scripts": "5.0.1",
    "styled-components": "^6.1.12",
    "typescript": "^5.5.4",
    "web-vitals": "^4.2.3"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
