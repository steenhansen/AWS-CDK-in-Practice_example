    "@types/node": "^22.4.1",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^8.3.4",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "superagent": "^8.0.0",
    "nodemon": "^3.1.4",
    "supertest": "^6.2.4",
    "ts-jest": "^29.2.4",
