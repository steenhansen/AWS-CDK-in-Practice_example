{
  "name": "server",
  "version": "1.0.0",
  "main": "src/server.js",
  "license": "MIT",
  "scripts": {
    "server-build": "gulp && tsc",
    "server-start": "gulp && cross-env AWS_SDK_JS_SUPPRESS_MAINTENANCE_MODE_MESSAGE=1 SERVER_LOCAL_MODE=yes nodemon --watch src src/index.ts",
    "server-test": "cross-env AWS_SDK_JS_SUPPRESS_MAINTENANCE_MODE_MESSAGE=1 SERVER_LOCAL_MODE=yes jest --detectOpenHandles",
    "server-test-AWS": "jest --detectOpenHandles",
    "test:coverage": "jest --watchAll --detectOpenHandles  --coverage",
    "forever": "forever --minUptime 5000 --spinSleepTime 5000 src/index.js"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.25.4",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.13",
    "@types/node": "^22.5.5",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "babel-jest": "^29.7.0",
    "cross-env": "^7.0.3",
    "gulp": "^5.0.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.7",
    "superagent": "^10.1.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.656.0",
    "@aws-sdk/client-rds-data": "^3.654.0",
    "@aws-sdk/client-secrets-manager": "^3.654.0",
    "@aws-sdk/lib-dynamodb": "^3.656.0",
    "@aws-sdk/types": "^3.654.0",
    "aws-sdk": "^2.1691.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "eslint": "^9.11.0",
    "eslint-plugin-prettier": "^5.2.1",
    "express": "^4.21.0",
    "forever": "^4.0.3",
    "prettier": "^3.3.3",
    "uuid": "^10.0.0"
  }
}